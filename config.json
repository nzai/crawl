						},{
							"type":"range",
							"width":4,
							"start":0,
							"end":{
								"index":6,
								"offset":-1
							},
							"parallel":8,
							"action":[{
								"type":"print",
								"pattern":"http://www.beautylegmm.com/photo/beautyleg/%s/%s/beautyleg-%s-%s.jpg",
								"param":[5,3,3,7]
							},{
								"type":"download",
								"url":{
									"pattern":"http://www.beautylegmm.com/photo/beautyleg/%s/%s/beautyleg-%s-%s.jpg",
									"param":[5,3,3,7]
								},
								"path":{
									"pattern":"G:\\BL\\%s\\%s %s\\%s.jpg",
									"param":[5,3,4,7]
								},
								"exists":"skip"
							}]
						}]
					}]
				}]
			}]
		}]
	}]
