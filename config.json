				"pattern":"目录一共有%s页",
				"param":[1]
			},{
				"type":"range",
				"width":1,
				"start":1,
				"end":{
					"index":1
				},
				"parallel":4,
				"action":[{
					"type":"print",
					"pattern":"下载第%s页",
					"param":[2]
				},{
					"type":"get",
					"url":{
						"pattern":"http://www.beautylegmm.com/index-%s.html",
						"param":[2]
					},
					"action":[{
						"type":"matches",
						"pattern":"<div align=\"center\" class=\"post_weidaopic_title\"><a href=\"([^\"]*?)\" target=\"_blank\">(.*?No.(\\d+) (\\w+) (\\d{4}).\\d{2}.\\d{2} \\[(\\d+)P\\])</a></div>",
						"action":[{
							"type":"print",
							"pattern":"%s\t%s\t%s\t%s\t%s\t%s",
