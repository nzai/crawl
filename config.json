				"pattern":"目录一共有%s页",
				"param":[1]
			},{
				"type":"range",
				"width":1,
				"start":1,
				"end":{
					"index":1
				},
				"action":[{
					"type":"get",
					"url:":{
						"pattern":"http://www.beautylegmm.com/index-%s.html",
						"param":[0]
					}
				}]
			}]
		}]
	}]
