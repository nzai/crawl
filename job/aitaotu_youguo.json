[{
    "type":"get",
    "parameters": {
        "url": {
            "type": "static",
            "value": "https://www.aitaotu.com/tag/youguowang.html"
        },
        "key": "tagPage",
        "debug": false
    },
    "actions":[{
        "type": "match",
        "parameters": {
            "key": "tagPage",
            "pattern": "(?s)/(\\d+).html\">末页</a>",
            "keys": ["tagPages"]
        },
        "actions":[{
            "type": "range",
            "parameters": {
                "start": {
                    "type": "static",
                    "value": 1
                },
                "end": {
                    "type": "context",
                    "key": "tagPages"
                },
                "format": "%d",
                "key": "tagPageIndex",
                "parallel": 8,
                "debug": false
            },
            "actions":[{
                "type":"get",
                "parameters": {
                    "url": {
                        "type": "format",
                        "pattern": "https://www.aitaotu.com/tag/youguowang/%s.html",
                        "keys": ["tagPageIndex"]
                    },
                    "key": "tagPageOne"
                },
                "actions":[{
                    "type": "match",
                    "parameters": {
                        "key": "tagPageOne",
                        "pattern": "(?s)class=\"Pli-litpic\" href=\"([^\"]*?)\" target",
                        "keys": ["sectionPageUrl"],
                        "debug": false
                    },
                    "actions":[{
                        "type":"get",
                        "parameters": {
                            "url": {
                                "type": "format",
                                "pattern": "https://www.aitaotu.com%s",
                                "keys": ["sectionPageUrl"]
                            },
                            "key": "sectionPageHtml"
                        },
                        "actions":[{
                            "type": "match",
                            "parameters": {
                                "key": "sectionPageHtml",
                                "pattern": "(?s)<h2>\\[[^\\]]*?\\].*?第(\\d+)期 .*? ([^<]*?)<\/h2>[\\s\\S]+?icons-addtime\"><\/i>发布时间：(\\d+)-(\\d+)-(\\d+)\\s",
                                "keys": ["sectionNo", "sectionGirl", "sectionYear","sectionMonth","sectionDay"],
                                "debug": true
                            }
                        }]
                    }]
                }]
            }]
        }]
    }]
}]