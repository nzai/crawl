[{
    "type": "range",
    "parameters": {
        "start": {
            "type": "static",
            "value": 1
        },
        "end": {
            "type": "static",
            "value": 100
        },
        "format": "%d",
        "key": "tagPageIndex",
        "parallel": 1,
        "debug": false
    },
    "actions":[{
        "type":"get",
        "parameters": {
            "url": {
                "type": "format",
                "pattern": "https://javmoo.net/cn/genre/d/page/%s",
                "keys": ["tagPageIndex"]
            },
            "key": "tagPage",
            "debug": false
        },
        "actions":[{
            "type": "match",
            "parameters": {
                "key": "tagPage",
                "pattern": "(?s)movie-box\" href=\"([^\"]+)\">[\\s\\S]*?<img src=\"(.*?)ps.jpg\"[\\s\\S]*?<date>([^<]+)<\/date> \/ <date>(\\d+)-(\\d+)-(\\d+)<",
                "keys": ["sectionPageUrl", "sectionImageUrl", "sectionCode", "sectionYear", "sectionMonth", "sectionDay"]
            },
            "actions":[{
                "type":"get",
                "parameters": {
                    "url": {
                        "type": "format",
                        "pattern": "%s",
                        "keys": ["sectionPageUrl"]
                    },
                    "key": "sectionPage",
                    "debug": false
                },
                "actions":[{
                    "type": "match",
                    "parameters": {
                        "key": "sectionPage",
                        "pattern": "(?s)sample-box\" href=\"(.*?-(\\d+\\.jpg))\"",
                        "keys": ["pictureUrl", "pictureFileName"]
                    },
                    "actions":[{
                        "type":"download",
                        "parameters": {
                            "url": {
                                "type": "format",
                                "pattern": "%s",
                                "keys": ["pictureUrl"]
                            },
                            "path": {
                                "type": "format",
                                "pattern": "/data/pic/javmoo/%s/%s-%s %s/%s",
                                "keys": ["sectionYear", "sectionMonth", "sectionDay", "sectionCode", "pictureFileName"]
                            },
                            "debug": false
                        }
                    }]
                }]
            }]
        }]
    }]
}]
